            <div class="content">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="card">
                                <div class="card-header card-header-primary card-header-icon">
                                    <div class="card-icon">
                                        <i class="material-icons">flight</i>
                                    </div>
                                    <h4 class="card-title">Modifier Vol</h4>
                                    <a href="{{ path('app_back_vol_index') }}" class="btn btn-warning btn-round float-right">
                                        <i class="material-icons">arrow_back</i> Retour à la liste
                                    </a>
                                </div>
                                <div class="card-body">
                                    {{ form_start(form, {'attr': {'class': 'form-horizontal'}}) }}
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="card card-nav-tabs">
                                                <div class="card-header card-header-info">
                                                    <h4 class="card-title">Informations du Vol</h4>
                                                </div>
                                                <div class="card-body">
                                                    <div class="row">
                                                        {% for field in form %}
                                                            {% if field.vars.name != '_token' %}
                                                                <div class="col-md-6">
                                                                    <div class="form-group bmd-form-group">
                                                                        {% if field.vars.name == 'classe' %}
                                                                            <div class="card">
                                                                                <div class="card-header card-header-warning">
                                                                                    <h4 class="card-title">
                                                                                        <i class="material-icons">business_class</i>
                                                                                        Classe de Vol
                                                                                    </h4>
                                                                                </div>
                                                                                <div class="card-body">
                                                                                    {{ form_widget(field, {'attr': {'class': 'form-control'}}) }}
                                                                                    {{ form_errors(field) }}
                                                                                </div>
                                                                            </div>
                                                                        {% else %}
                                                                            <div class="card">
                                                                                <div class="card-body">
                                                                                    {{ form_label(field, null, {'label_attr': {'class': 'bmd-label-floating'}}) }}
                                                                                    {{ form_widget(field, {'attr': {'class': 'form-control'}}) }}
                                                                                    {{ form_errors(field) }}
                                                                                </div>
                                                                            </div>
                                                                        {% endif %}
                                                                    </div>
                                                                </div>
                                                            {% endif %}
                                                        {% endfor %}
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="card-footer ml-auto mr-auto text-center">
                                        <button type="submit" class="btn btn-primary btn-round">
                                            <i class="material-icons">save</i> Enregistrer
                                        </button>
                                    </div>
                                    {{ form_end(form) }}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
