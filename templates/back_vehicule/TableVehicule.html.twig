			<div class="content">
				<div class="container-fluid">
					<div class="row">
						<div class="col-md-12">
							<div class="card">
								<div class="card-header card-header-primary card-header-icon card-header-warning">
									<div class="card-icon">
										<i class="material-icons">directions_car</i>
									</div>
									<h4 class="card-title">Tableau des Véhicules</h4>
								</div>
								<div class="card-body">
									<div class="toolbar">
										<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#addVehicleModal">Ajouter un véhicule</button>
									</div>
									<div class="material-datatables">
										<table id="datatables" class="table table-striped table-no-bordered table-hover" cellspacing="0" width="100%" style="width:100%">
											<thead>
												<tr>
													<th>Matricule</th>
													<th>Statut</th>
													<th>Nombre de places</th>
													<th>Cylindre</th>
													<th class="disabled-sorting text-right">Actions</th>
												</tr>
											</thead>
											<tfoot>
												<tr>
													<th>Matricule</th>
													<th>Statut</th>
													<th>Nombre de places</th>
													<th>Cylindre</th>
													<th class="text-right">Actions</th>
												</tr>
											</tfoot>
											<tbody>
												{% for vehicule in vehicules %}
													<tr data-id="{{ vehicule.getId_vehicule() }}">
														<td>{{ vehicule.matricule }}</td>
														<td>{{ vehicule.status }}</td>
														<td>{{ vehicule.nbPlace }}</td>
														<td>{{ vehicule.cylinder }}</td>
														<td class="text-right">
															<a href="#" class="btn btn-link btn-warning btn-just-icon edit-vehicle" title="Modifier" data-id="{{ vehicule.getId_vehicule() }}" data-matricule="{{ vehicule.matricule }}" data-status="{{ vehicule.status }}" data-nbplace="{{ vehicule.nbPlace }}" data-cylinder="{{ vehicule.cylinder }}">
																<i class="material-icons">edit</i>
															</a>
															<a href="#" class="btn btn-link btn-danger btn-just-icon remove" title="Supprimer" data-url="{{ path('app_back_vehicule_delete', {'id': vehicule.getId_vehicule()}) }}">
																<i class="material-icons">close</i>
															</a>
														</td>
													</tr>
												{% endfor %}
											</tbody>
										</table>
									</div>

									<!-- Modal for Adding a Vehicle -->
									<div class="modal fade" id="addVehicleModal" tabindex="-1" role="dialog" aria-labelledby="addVehicleModalLabel" aria-hidden="true">
										<div class="modal-dialog" role="document">
											<div class="modal-content">
												<div class="modal-header">
													<h5 class="modal-title" id="addVehicleModalLabel">Ajouter un véhicule</h5>
													<button type="button" class="close" data-dismiss="modal" aria-label="Close">
														<span aria-hidden="true">×</span>
													</button>
												</div>
												<div class="modal-body">
													<form id="addVehicleForm" data-url="{{ path('app_back_vehicule_new') }}" novalidate>
														<div class="form-group bmd-form-group">
															<label for="matricule" class="bmd-label-floating">Matricule</label>
															<input type="text" class="form-control" id="matricule" name="matricule" pattern="\d{1,4}TUN\d{1,4}">
														</div>
														<div class="form-group bmd-form-group">
															<label for="status" class="bmd-label-floating">Statut</label>
															<input type="text" class="form-control" id="status" name="status" pattern="[A-Za-z]+">
														</div>
														<div class="form-group bmd-form-group">
															<label for="nbPlace" class="bmd-label-floating">Nombre de places</label>
															<input type="number" class="form-control" id="nbPlace" name="nbPlace" min="0">
														</div>
														<div class="form-group bmd-form-group">
															<label for="cylinder" class="bmd-label-floating">Cylindre</label>
															<input type="number" class="form-control" id="cylinder" name="cylinder" min="0">
														</div>
													</form>
												</div>
												<div class="modal-footer">
													<button type="button" class="btn btn-secondary" data-dismiss="modal">Annuler</button>
													<button type="button" class="btn btn-primary" id="submitVehicle">Ajouter</button>
												</div>
											</div>
										</div>
									</div>

									<!-- Modal for Editing a Vehicle -->
									<div class="modal fade" id="editVehicleModal" tabindex="-1" role="dialog" aria-labelledby="editVehicleModalLabel" aria-hidden="true">
										<div class="modal-dialog" role="document">
											<div class="modal-content">
												<div class="modal-header">
													<h5 class="modal-title" id="editVehicleModalLabel">Modifier un véhicule</h5>
													<button type="button" class="close" data-dismiss="modal" aria-label="Close">
														<span aria-hidden="true">×</span>
													</button>
												</div>
												<div class="modal-body">
													<form id="editVehicleForm" novalidate>
														<input type="hidden" id="editVehicleId" name="id">
														<div class="form-group bmd-form-group">
															<label for="editMatricule" class="bmd-label-floating">Matricule</label>
															<input type="text" class="form-control" id="editMatricule" name="matricule" pattern="\d{1,4}TUN\d{4}">
														</div>
														<div class="form-group bmd-form-group">
															<label for="editStatus" class="bmd-label-floating">Statut</label>
															<input type="text" class="form-control" id="editStatus" name="status" pattern="[A-Za-z]+">
														</div>
														<div class="form-group bmd-form-group">
															<label for="editNbPlace" class="bmd-label-floating">Nombre de places</label>
															<input type="number" class="form-control" id="editNbPlace" name="nbPlace" min="0">
														</div>
														<div class="form-group bmd-form-group">
															<label for="editCylinder" class="bmd-label-floating">Cylindre</label>
															<input type="number" class="form-control" id="editCylinder" name="cylinder" min="0">
														</div>
													</form>
												</div>
												<div class="modal-footer">
													<button type="button" class="btn btn-secondary" data-dismiss="modal">Annuler</button>
													<button type="button" class="btn btn-primary" id="submitEditVehicle">Modifier</button>
												</div>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
