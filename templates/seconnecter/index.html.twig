	<div class="signup-wrapper space">
		<div class="container">
			<div class="row justify-content-center">
				<div class="col-lg-6">
					<form action="{{ path('app_seconnecter') }}" class="signup-form bg-smoke" method="post" novalidate>

						<h2 class="form-title text-center mb-lg-35">Se connecter</h2>
						<div class="form-group">
							<label for="loginUserId" class="sr-only">Adresse e-mail*</label>
							<input type="text" class="form-control" placeholder="Adresse e-mail*" id="loginUserId" name="mail" value="{{last_username}}"/>
						</div>
						<div class="form-group">
							<label for="loginUserPass" class="sr-only">Mot de passe*</label>
							<input type="password" class="form-control" placeholder="Mot de passe*" id="loginUserPass" name="mdp"/>
						</div>
						<div class="form-group">
							<input type="checkbox" name="loginRemember" id="loginRemember"/>
						</div>
						<div class="form-group mb-0 text-center">
							<button class="vs-btn mask-style1 w-100 style4" type="submit">
								Se connecter
							</button>
							<div class="bottom-links link-inherit d-md-flex justify-content-between mt-3">
								<a href="#" class="recovery-link mb-2 mb-md-0">Mot de passe oublié ?</a>
								<a href="{{ path('app_sinscrire') }}">Ou Créer un compte</a>
							</div>
						</div>

					</form>
				</div>
			</div>
